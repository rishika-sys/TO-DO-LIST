<%- include('header'); -%>
    <div class="box" id="heading">
 <h1>Its a <%= listTitle %></h1>
    </div>

  <div class="box">
    <% for(var i=0;i<newitems.length;i++){ %>
      <div class="item">
  <input type="checkbox">
      <p><%= newitems[i] %></p>
    </div>
   <% }%>
   <form class="item" method="POST" action="/">
     <input type="text" name="value" placeholder="New Item" autocomplete="off"></input>
     <button type="submit" name="list" value=<%= listTitle %>>+</button>
   </form>
